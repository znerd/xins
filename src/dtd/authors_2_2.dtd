<!--
 XINS Type DTD. This DTD should be added to the authors.xml file by adding the
 following lines at the top of the file :
<!DOCTYPE authors PUBLIC "-//XINS//DTD Authors 2.2//EN" "http://www.xins.org/dtd/authors_2_2.dtd">
-->
<!ELEMENT authors (author*)>
<!ELEMENT author EMPTY>
<!ATTLIST author
          id         NMTOKEN #REQUIRED
          email      NMTOKEN #REQUIRED
          name       CDATA   #REQUIRED
>

